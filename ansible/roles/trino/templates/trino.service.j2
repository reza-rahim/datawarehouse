[Unit]
Description=Trino
After=network.target keycloak.service jupyterhub.service

[Service]
User={{ trino_user }}
Group={{ application_group }}

Environment=JAVA_HOME={{ java_home }}

ExecStart=/bin/bash -c "eval $({{ lockbox_dir }}/decrypt_lockbox_eval.sh);{{ trino_home }}/bin/launcher run"
Restart=on-failure


[Install]
WantedBy=multi-user.target
